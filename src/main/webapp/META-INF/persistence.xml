<?xml version="1.0" encoding="UTF-8" ?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
 http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd" version="1.0">

    <persistence-unit name="fxl-pu">
        <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
        <class>net.feminaexlux.model.User</class>
        <properties>
            <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3333/shimmers?useUnicode=true&amp;useJDBCCompliantTimezoneShift=true&amp;useLegacyDatetimeCode=false&amp;serverTimezone=PST"/>
            <property name="javax.persistence.jdbc.user" value="shimmers"/>
            <property name="javax.persistence.jdbc.password" value="shimmers"/>
        </properties>
    </persistence-unit>

</persistence>
